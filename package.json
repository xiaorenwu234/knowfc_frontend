{
  "name": "knowfc_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "copy-webviewer": "mkdir -p ./public/lib/webviewer && cp -a ./node_modules/@pdftron/webviewer/public/* ./public/lib/webviewer",
    "dev": "npm run copy-webviewer && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "npm run copy-webviewer && vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@pdftron/webviewer": "^11.5.0",
    "@stomp/stompjs": "^7.1.1",
    "axios": "^1.10.0",
    "cropperjs": "^1.6.2",
    "heroicons": "^2.2.0",
    "iconify": "^1.4.0",
    "jquery": "^3.7.1",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "tailwindcss": "3.4.17",
    "vditor": "^3.11.1",
    "vis": "^4.21.0",
    "vue": "^3.5.13",
    "vue-cropperjs": "^5.0.0",
    "vue-router": "^4.5.0",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.351",
    "@iconify/tailwind": "^0.1.4",
    "@tsconfig/node22": "^22.0.1",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@types/sockjs-client": "^1.5.4",
    "@types/vue-cropperjs": "^4.1.6",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.7.3",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.4.38",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
